
@{
    ViewData["Title"] = "car_information";
    var list = ViewData["car_inf"] as List<AutomobileSalesSystem.Entitys.Car>;
}

<div class="content-page">

    <!-- Start content -->
    <div class="content">

        <div class="container-fluid">

            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xl-6">
                <div class="card mb-3">
                    <div class="card-header">
                        <h3><i class="fa fa-table"></i>汽车信息</h3>
                    </div>

                    <div class="card-body">
                        <table class="table table-responsive-xl table-striped">
                            <thead>
                                <tr>
                                    <th scope="col">车辆编号</th>
                                    <th scope="col">车架型号</th>
                                    <th scope="col">车辆类型</th>
                                    <th scope="col">车辆厂家</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in list)
                                {
                                    <tr>
                                        <th scope="row">@item.Id</th>
                                        <td>@item.Model</td>
                                        <td>@item.Type</td>
                                        <td>@item.Factory</td>
                                    </tr>
                                }


                            </tbody>
                        </table>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
