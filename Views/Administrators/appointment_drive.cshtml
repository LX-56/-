
@{
    ViewData["Title"] = "appointment_drive";
    var list = ViewData["appoint_inf"] as List<AutomobileSalesSystem.Entitys.Drivetest>;
}

<div class="content-page">

    <!-- Start content -->
    <div class="content">

        <div class="container-fluid">

            <div class="row">

                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xl-6">
                    <div class="card mb-3">
                        <div class="card-header">
                            <h3><i class="fa fa-table"></i> 试驾预约</h3>

                        </div>

                        <div class="card-body">

                            <table class="table table-responsive-xl">
                                <thead>
                                    <tr>
                                        <th scope="col">用户编号</th>
                                        <th scope="col">车辆编号</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in list)
                                    {
                                        <tr>
                                            <th scope="row">@item.Cid</th>
                                            <td>@item.Aid</td>

                                        </tr>
                                    }


                                </tbody>
                            </table>

                        </div>
                    </div><!-- end card-->
                </div>

            </div>
        </div>
    </div>
</div>